<!DOCTYPE html
  PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">

<head>
  <meta charset="UTF-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  <title>Shakti Pumps</title>
  <link rel="icon" type="image/x-icon" href="/img/fav.ico" />
  <link rel="preconnect" href="https://fonts.googleapis.com" />
  <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />
  <link
    href="https://fonts.googleapis.com/css2?family=Poppins:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,100;1,200;1,300;1,400;1,500;1,600;1,700;1,800;1,900&display=swap"
    rel="stylesheet" />
  <script type="text/javascript" src="/js/jquery.min.js"></script>
  <script type="text/javascript" src="/js/bootstrap.bundle.min.js"></script>
  <link rel="stylesheet" href="/css/bootstrap.min.css" />
  <link rel="stylesheet" href="https://unpkg.com/boxicons@2.0.7/css/boxicons.min.css" />
  <link rel="stylesheet" href="/css/style.css" />
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/normalize/5.0.0/normalize.min.css" />
  <link rel="stylesheet" href="/css/style-loading.css" />
  <link rel="stylesheet" href="/css/responsive.css" />
</head>

<body>
  <%- include('../common/header.ejs') %>

    <section class="newsfilter newsdtlshead">
      <div class="container">
        <div class="row">
          <div class="col-lg-12">
            <h2>
              <%= eventDetails.event_name %>
            </h2>
            <h3>
              <span><img src="/images/calendar.png" alt="" />
                <%= eventDetails.event_frm_date %>
              </span>
              <span><img src="/images/news-icon.png" alt="" height="24" /> Times Of
                India</span>
            </h3>
          </div>
        </div>
      </div>
    </section>
    <section class="newsdetailsdesc">
      <div class="container">

        <% for(let i=0; i<paragraphArray.length; i++){ %>
          <p>
            <%= paragraphArray[i] %>
          </p>
          <% } %>
            </paragraphArray.length;>
      </div>

    </section>

    <section class="imgevent">

      <div class="container">
        <div class="row">
          <div class="col-sm-4 col-md-4 col-lg-4 ">
            <img src="/images/events/subImages/<%= eventDetails.sub_image1%>" alt="" class="ndtlsthum" />
          </div>
          <div class="col-sm-4 col-md-4 col-lg-4 ">
            <img src="/images/events/subImages/<%= eventDetails.sub_image2%>" alt="" class="ndtlsthum" />
          </div>
          <div class="col-sm-4 col-md-4 col-lg-4 ">
            <img src="/images/events/subImages/<%= eventDetails.sub_image3%>" alt="" class="ndtlsthum" />
          </div>
        </div>
        <div class="row">
          <div class="col-sm-4 col-md-4 col-lg-4 ">
            <img src="/images/events/subImages/<%= eventDetails.sub_image4%>" alt="" class="ndtlsthum" />
          </div>
          <div class="col-sm-4 col-md-4 col-lg-4 ">
            <img src="/images/events/subImages/<%= eventDetails.sub_image5%>" alt="" class="ndtlsthum" />
          </div>
          <div class="col-sm-4 col-md-4 col-lg-4 ">
            <img src="/images/events/subImages/<%= eventDetails.actual_image%>" alt="" class="ndtlsthum" />
          </div>
        </div>
      </div>

    </section>


    <%- include('../common/footer.ejs') %>
</body>

</html>