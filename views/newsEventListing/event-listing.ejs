<section class="newsfilter">
<div class="container">
  <div class="row">
    <div class="col-lg-8 col-md-8 col-sm-12">
      <h2>Shakti Pumps Events
        <span>Total events: <%= Array.isArray(allEvents) ? allEvents.length : 0 %></span>
      </h2>
    </div>
    <div class="col-lg-4 col-md-4 col-sm-12 newfilrgt">
      <select>
        <option>Select Category</option>
      </select>
      <input type="date" value="DD.MM.YYYY">
    </div>
  </div>
</div>
</section>
