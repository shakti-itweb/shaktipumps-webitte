    <div class="tab_container">
      <h3 class="d_active tab_drawer_heading" rel="tab1">Outcome of Board Meeting</h3>
      <div id="tab1" class="tab_content">
        <div class="foundtabinsid financedtlstab">
          <div class="financedtlstitle">
            <h2>Outcome of Board Meeting <span>Outcome of Board Meeting Report of Shakti Pumps (I) Ltd.</span></h2>
            <a href="#" class="resultsbtn"><img src="/images/pdf-icon.png" alt=""> Report Download: 2023-24</a>
          </div>
          <div class="downloadcardlist">
            <div class="downloadcardpan">
              <h3>Select Financial Year <span>Credit Rating Report of Shakti Pumps (I) Ltd.</span></h3>
              <a href="#" class="readmore"><img src="/images/pdf-iconwh.png" alt=""> Download</a>
            </div>
            <% if (Array.isArray(OutcomeofBoardMeeting.recordset) && OutcomeofBoardMeeting.recordset.length > 0) {OutcomeofBoardMeeting.recordset.forEach(function(file) { %>
              <div class="downloadcardpan">
                <h3><%=file.fileName %> <span><%=file.quater%> Annual Report of Shakti Pumps (I) Ltd.</span></h3>
                <a href="/pdfFiles/boardMeeting/OutcomeofBoardMeeting/<%=file.path%>" class="readmore" target="_blank"><img src="/images/pdf-iconwh.png" alt=""> Download</a>
              </div>
              <% })} %>
          </div>        
        </div>
      </div>
     
      <h3 class="tab_drawer_heading" rel="tab2">
        Intimation of Board Meeting</h3>
      <div id="tab2" class="tab_content">
        <div class="foundtabinsid financedtlstab">
