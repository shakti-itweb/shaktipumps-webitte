      rel="stylesheet"
      href="https://cdnjs.cloudflare.com/ajax/libs/normalize/5.0.0/normalize.min.css"
    />
    <link rel="stylesheet" href="/css/style-loading.css" />
    <link rel="stylesheet" href="/css/responsive.css" />
  </head>
  <body>
    <%- include('../common/header.ejs') %>

    <section class="about-banner lifeatsktban">
      <div><img src="/images/financial-banner.png" alt="" /></div>
      <div class="container">
        <h2>
          Financial<br />
          Report
        </h2>
      </div>
    </section>
    <section class="foundtiontab financialyeartab">
      <div class="container">
        <div class="selectcat">
          <select onchange="getval(this.value);">
            <option>ALL</option>
            <option value="2024-2025">2024-2025</option>
            <option value="2023-2024">2023-2024</option>
          </select>
        </div>
        <!-- partial:index.partial.html -->
        <ul class="tabs">
          <li class="active" rel="tab1">
            Quarterly Results <span>Consectetur Adipiscing Elit</span>
          </li>
          <li rel="tab2">Annual Report <span>Shakti Pumps India Ltd.</span></li>
          <li rel="tab3">
            Annual Return <span>Consectetur Adipiscing Elit</span>
          </li>
          <li rel="tab4">
            Investor Presentation <span>Consectetur Adipiscing Elit</span>
          </li>
          <li rel="tab5">
            Conference Call <span>Consectetur Adipiscing Elit</span>
          </li>
          <li rel="tab6">
            Related Party Disclosure <span>Consectetur Adipiscing Elit</span>
          </li>
          <li rel="tab7">
            Subsidiary Results <span>Consectetur Adipiscing Elit</span>
          </li>
        </ul>
        <div class="tab_container">
          <h3 class="d_active tab_drawer_heading" rel="tab1">
            Quarterly Results
          </h3>
          <div id="tab1" class="tab_content">
            <div class="foundtabinsid financedtlstab">
              <div class="financedtlstitle">
                <h2>
                  Annual Report
                  <span>Annual Report of Shakti Pumps (I) Ltd.</span>
                </h2>
                <a href="#" class="resultsbtn"
                  ><img src="/images/pdf-icon.png" alt="" /> Report Download:
                  2023-24</a
                >
              </div>
              <div class="downloadcardlist">
                <% if (Array.isArray(quarterlyResults.recordset) &&
                quarterlyResults.recordset.length > 0)
                {quarterlyResults.recordset.forEach(function(file) { %>
                <div class="downloadcardpan" id="<%=file.year %>">
                  <h3>
                    <%=file.fileName %><span
                      ><%=file.quater%> Annual Report of Shakti Pumps (I)
                      Ltd.</span
                    ><span><%=file.year %></span>
                  </h3>
                  <!-- <h3><span><%=file.year %></span></h3> -->
                  <a
                    href="/pdfFiles/financialReports/qualityResults/<%=file.path%>"
                    class="readmore"
                    target="_blank"
                    ><img src="/images/pdf-iconwh.png" alt="" /> Download</a
                  >
                </div>
                <% })} %>
              </div>
            </div>
          </div>
          <!-- #tab1 -->
          <h3 class="tab_drawer_heading" rel="tab2">Annual Report</h3>
          <div id="tab2" class="tab_content">
            <div class="foundtabinsid financedtlstab">
              <div class="financedtlstitle">
                <h2>
                  Annual Report
                  <span>Annual Report of Shakti Pumps (I) Ltd.</span>
                </h2>
                <a href="#" class="resultsbtn"
                  ><img src="/images/pdf-icon.png" alt="" /> Report Download:
                  2023-24</a
                >
              </div>
              <div class="downloadcardlist">
                <% if (Array.isArray(annualReport.recordset) &&
                annualReport.recordset.length > 0)
                {annualReport.recordset.forEach(function(file) { %>
                <div class="downloadcardpan" id="<%=file.year %>">
                  <h3>
                    <%=file.fileName %>
                    <span
                      ><%=file.year%> Annual Report of Shakti Pumps (I)
                      Ltd.</span
                    >
                  </h3>
                  <a
                    href="/pdfFiles/financialReports/annualReport/<%=file.path%>"
                    class="readmore"
                    target="_blank"
                    ><img src="/images/pdf-iconwh.png" alt="" /> Download</a
                  >
                </div>
                <% })} %>
              </div>
            </div>
            <div class="foundtabinsid financedtlstab">
              <div class="financedtlstitle">
                <h2>BRSR <span>BRSR of Shakti Pumps (I) Ltd.</span></h2>
                <a href="#" class="resultsbtn"
                  ><img src="/images/pdf-icon.png" alt="" /> Report Download:
                  2023-24</a
                >
              </div>
              <div class="downloadcardlist">
                <% if (Array.isArray(BRSR.recordset) && BRSR.recordset.length >
                0) {BRSR.recordset.forEach(function(file) { %>
                <div class="downloadcardpan" id="<%=file.year %>">
                  <h3>
                    <%=file.fileName %>
                    <span
                      ><%=file.year%> Annual Report of Shakti Pumps (I)
                      Ltd.</span
                    >
                  </h3>
                  <a
                    href="/pdfFiles/financialReports/BRSR/<%=file.path%>"
                    class="readmore"
                    target="_blank"
                    ><img src="/images/pdf-iconwh.png" alt="" /> Download</a
                  >
                </div>
                <% })} %>
              </div>
            </div>
          </div>
          <h3 class="tab_drawer_heading" rel="tab3">Annual Returns</h3>
          <div id="tab3" class="tab_content">
            <div class="foundtabinsid financedtlstab">
              <div class="financedtlstitle">
                <h2>
                  Annual Return
                  <span>Annual Return of Shakti Pumps (I) Ltd.</span>
                </h2>
                <a href="#" class="resultsbtn"
                  ><img src="/images/pdf-icon.png" alt="" /> Report Download:
                  2023-24</a
                >
              </div>
              <div class="downloadcardlist">
                <% if (Array.isArray(annualReturns.recordset) &&
                annualReturns.recordset.length > 0)
                {annualReturns.recordset.forEach(function(file) { %>
                <div class="downloadcardpan" id="<%=file.year %>">
                  <h3>
                    <%=file.fileName %>
                    <span
                      ><%=file.year%> Annual Report of Shakti Pumps (I)
                      Ltd.</span
                    >
                  </h3>
                  <a
                    href="/pdfFiles/financialReports/annualReturns/<%=file.path%>"
                    class="readmore"
                    target="_blank"
                    ><img src="/images/pdf-iconwh.png" alt="" /> Download</a
                  >
                </div>
                <% })} %>
              </div>
            </div>
          </div>
          <h3 class="tab_drawer_heading" rel="tab4">Investor Presentation</h3>
          <div id="tab4" class="tab_content">
            <div class="foundtabinsid financedtlstab">
              <div class="financedtlstitle">
                <h2>
                  Result Release<span
                    >Result Release of Shakti Pumps (I) Ltd.</span
                  >
                </h2>
                <a href="#" class="resultsbtn"
                  ><img src="/images/pdf-icon.png" alt="" /> Report Download:
                  2023-24</a
                >
              </div>
              <div class="downloadcardlist">
                <% if (Array.isArray(resultRelease.recordset) &&
                resultRelease.recordset.length > 0)
                {resultRelease.recordset.forEach(function(file) { %>
                <div class="downloadcardpan" id="<%=file.year %>">
                  <h3>
                    <%=file.fileName %>
                    <span
                      ><%=file.year%> Annual Report of Shakti Pumps (I)
                      Ltd.</span
                    >
                  </h3>
                  <a
                    href="/pdfFiles/financialReports/resultRelease/<%=file.path%>"
                    class="readmore"
                    target="_blank"
                    ><img src="/images/pdf-iconwh.png" alt="" /> Download</a
                  >
                </div>
                <% })} %>
              </div>
            </div>
            <div class="foundtabinsid financedtlstab">
              <div class="financedtlstitle">
                <h2>
                  Investor Presentation
                  <span>Investor Presentation of Shakti Pumps (I) Ltd.</span>
                </h2>
                <a href="#" class="resultsbtn"
                  ><img src="/images/pdf-icon.png" alt="" /> Report Download:
                  2023-24</a
                >
              </div>
              <div class="downloadcardlist">
                <% if (Array.isArray(resultRelease.recordset) &&
                resultRelease.recordset.length > 0)
                {resultRelease.recordset.forEach(function(file) { %>
                <div class="downloadcardpan" id="<%=file.year %>">
                  <h3>
                    <%=file.fileName %>
                    <span
                      ><%=file.year%> Annual Report of Shakti Pumps (I)
                      Ltd.</span
                    >
                  </h3>
                  <a
                    href="/pdfFiles/financialReports/resultRelease/<%=file.path%>"
                    class="readmore"
                    target="_blank"
                    ><img src="/images/pdf-iconwh.png" alt="" /> Download</a
                  >
                </div>
                <% })} %>
              </div>
            </div>
          </div>
        </div>

        <h3 class="tab_drawer_heading" rel="tab6">Related Party Disclosure</h3>
        <div id="tab6" class="tab_content">
          <div class="foundtabinsid financedtlstab">
            <div class="financedtlstitle">
              <h2>
                Related Party Disclosure
                <span>Related Party Disclosure of Shakti Pumps (I) Ltd.</span>
              </h2>
              <a href="#" class="resultsbtn"
                ><img src="/images/pdf-icon.png" alt="" /> Report Download:
                2023-24</a
              >
            </div>
            <div class="downloadcardlist">
              <% if (Array.isArray(relatedPartyDisclosure.recordset) &&
              relatedPartyDisclosure.recordset.length > 0)
              {relatedPartyDisclosure.recordset.forEach(function(file) { %>
              <div class="downloadcardpan" id="<%=file.year %>">
                <h3>
                  <%=file.fileName %>
                  <span
                    ><%=file.path%> Annual Report of Shakti Pumps (I) Ltd.</span
                  >
                </h3>
                <a
                  href="/pdfFiles/financialReports/relatedPartyDisclosure/<%=file.path%>"
                  class="readmore"
                  target="_blank"
                  ><img
                    src="/pdfFiles/financialReports/relatedPartyDisclosure/<%=file.path%>"
                    alt=""
                  />
                  Download</a
                >
              </div>
              <% })} %>
            </div>
          </div>
        </div>
      </div>
      <!-- .tab_container -->
    </section>
    <%- include('../common/footer.ejs') %>
  </body>
</html>
