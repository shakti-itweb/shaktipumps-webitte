             <div class="prerelboxcard">

                <% if(Array.isArray(financialYear24) && financialYear24.length > 0 ){ %>
                    <%  financialYear24.map((val)=>{ %>
                      <div class="lifrresoucecard pressrelcard">
                        <img src="<%= val.image_path %><%= val.image_name%>" alt="">
                        <h4><%= val.image_description %></h4>
                        <h6><img src="/images/calendar.png" alt=""> <%= val.createdOn %></h6>
                      </div>
                      <%})%>
                <% } %>
             </div>
        </div>
      </div>
